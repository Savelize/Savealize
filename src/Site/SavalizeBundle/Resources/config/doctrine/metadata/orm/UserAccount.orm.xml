<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserAccount" table="user_account">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fname" type="string" column="fname" length="30" nullable="false"/>
    <field name="lname" type="string" column="lname" length="30" nullable="false"/>
    <field name="username" type="string" column="username" length="20" nullable="false"/>
    <field name="password" type="string" column="password" length="30" nullable="false"/>
    <field name="email" type="string" column="email" length="40" nullable="false"/>
    <field name="country" type="string" column="country" length="30" nullable="false"/>
    <field name="city" type="string" column="city" length="30" nullable="false"/>
    <field name="region" type="string" column="region" length="30" nullable="false"/>
    <field name="age" type="integer" column="age" nullable="false"/>
    <field name="martalStatus" type="string" column="martal_status" length="15" nullable="false"/>
    <field name="car" type="blob" column="car" nullable="false"/>
    <field name="salary" type="integer" column="salary" nullable="false"/>
    <field name="createdAt" type="date" column="created_at" nullable="false"/>
    <field name="updatedAt" type="date" column="updated_at" nullable="false"/>
    <many-to-one field="linkedTo" target-entity="UserAccount">
      <join-columns>
        <join-column name="linked_to" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
