<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ProductRating" table="product_rating">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id" nullable="true"/>
    <field name="productBrandId" type="integer" column="product_brand_id" nullable="false"/>
    <field name="rating" type="integer" column="rating" nullable="false"/>
    <field name="liked" type="boolean" column="liked" nullable="false"/>
  </entity>
</doctrine-mapping>
