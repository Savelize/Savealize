<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdminAccount" table="admin_account">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="30" nullable="false"/>
    <field name="username" type="string" column="username" length="20" nullable="false"/>
    <field name="email" type="string" column="email" length="30" nullable="false"/>
    <field name="password" type="string" column="password" length="20" nullable="false"/>
    <field name="isDeleted" type="blob" column="is_deleted" nullable="false"/>
  </entity>
</doctrine-mapping>
