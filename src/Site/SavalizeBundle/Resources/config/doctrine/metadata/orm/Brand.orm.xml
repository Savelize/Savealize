<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Brand" table="brand">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="30" nullable="false"/>
    <field name="confirmed" type="blob" column="confirmed" nullable="false"/>
    <field name="isDeleted" type="blob" column="is_deleted" nullable="false"/>
    <many-to-many field="product" target-entity="Product" mapped-by="brand"/>
  </entity>
</doctrine-mapping>
